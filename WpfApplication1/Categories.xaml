<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApplication1"
        xmlns:DomainClasses="clr-namespace:DomainClasses;assembly=DomainClasses" x:Class="WpfApplication1.Categories"
        mc:Ignorable="d"
        Title="Categories" Height="151.247" Width="162.881">
    <Grid Background="#FFD1D3FF">
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="101" Margin="10,10,0,0" VerticalAlignment="Top" Width="135" ItemsSource="{Binding Categories}">
            <ListBox.DataContext>
                <DomainClasses:ViewModel/>
            </ListBox.DataContext>
        </ListBox>
    </Grid>
</Window>
